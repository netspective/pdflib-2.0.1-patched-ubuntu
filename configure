	Linux*) CCSHARED="-fpic";;
	SCO_SV*) CCSHARED="-Kpic -belf";;
	FreeBSD*) CCSHARED="-fpic";;
	NetBSD*) CCSHARED="-fpic";;
	cyg*|CYG*) CCSHARED="-DBUILDING_DLL=1";;
	esac
fi
echo "$ac_t""$CCSHARED" 1>&6

